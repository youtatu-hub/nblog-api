<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.net.perfect.storage.domain.log.mapper.Dt50BaseNewLogMapper">

    <select id="streamingQueryContinuousInfo" resultType="cn.net.perfect.storage.domain.log.po.Dt50BaseNewLog">
        select insert_time as insertTime, log_time as logTime, tid as tid, groupId as groupId from dt50_log
        <where>
            <if test="null != tid">
                and tid = #{tid}
            </if>
            <if test="null != startTime">
                <![CDATA[ AND log_time >= #{startTime} ]]>
            </if>
            <if test="null != endTime">
                <![CDATA[ AND log_time <= #{endTime} ]]>
            </if>
            order by log_time asc
        </where>
    </select>

    <select id="selectMaxSequNo" resultType="java.lang.Long">
        select MAX(sequ_no) FROM dt50_log
    </select>

</mapper>